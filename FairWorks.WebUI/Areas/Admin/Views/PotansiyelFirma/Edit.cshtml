                <input asp-for="Sehir" class="form-control" />
                <span asp-validation-for="Sehir" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sektor" class="control-label"></label>
                <input asp-for="Sektor" class="form-control" />
                <span asp-validation-for="Sektor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
            <div>
                 <a asp-action="Index">Listeye Dön</a>
            </div>

        </form>
    </div>
</div>



