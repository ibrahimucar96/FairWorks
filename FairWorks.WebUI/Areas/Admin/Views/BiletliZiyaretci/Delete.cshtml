@model BiletliZiyaretci

<form method="post" asp-controller="BiletliZiyaretci" asp-action="Delete">
   
    <div class="form-group row">
        <label asp-for="Ad" class="col-md-2 col-form-label">@Model.Ad</label>        
    </div>
    <div class="form-group row">
        <label asp-for="SoyAd" class="col-md-2 col-form-label">@Model.SoyAd</label>        
    </div>
    <div class="form-group row">
        <label asp-for="FirmaAdı" class="col-md-2 col-form-label">@Model.FirmaAdı</label>        
    </div>
    <div class="form-group row">
        <label asp-for="Adres" class="col-md-2 col-form-label">@Model.Adres</label>        
    </div>
    <div class="form-group row">
        <label asp-for="Telefon" class="col-md-2 col-form-label">@Model.Telefon</label>        
    </div>
    <div class="form-group row">
        <label asp-for="Eposta" class="col-md-2 col-form-label">@Model.Eposta</label>        
    </div>
    <div class="form-group row">
        <label asp-for="Faks" class="col-md-2 col-form-label">@Model.Faks</label>        
    </div>
    <div class="form-group row">
        <label asp-for="Meslek" class="col-md-2 col-form-label">@Model.Meslek</label>        
    </div>    
    <p>Bu kaydi Silmek istediginizden eminmisiniz ?</p>
    <div class="form-group row">
        <button type="submit" class="btn btn-primary">Sil</button>
        <a  class="btn btn-secondary" asp-action="Index" asp-controller="BiletliZiyaretci">Listeye Geri Dön</a>
    </div>
</form>